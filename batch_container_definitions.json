[
    {
        "name": "alpine",
        "image": "alpine:latest",
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "ap-northeast-1",
                "awslogs-stream-prefix": "alpine",
                "awslogs-group": "/ecs-scheduled_tasks/default"
            }
        },
        "command": [
            "/bin/date"
        ]
    }
]